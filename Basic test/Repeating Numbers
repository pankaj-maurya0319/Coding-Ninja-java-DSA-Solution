
public class solution {

	public static void print(int n) {
		
        int i=1;
        int j=1;
        int print = 1;
        while(i<=n){
            
            int k=1;
            
        	while(k<=print){
                System.out.print(j++);
                if(j==10){
                    j=1;
                }
                
                k++;
            }    
            print=print*2;
            System.out.println();
            i++;
        }
        
	}

}
