
public class Solution {

	public static void printSubstrings(String str) {
		//Your code goes here
        int n = str.length();
        
        for(int i=0; i<n; i++){
            int j=i;
           
            while(j<n){
                System.out.println(str.substring(i,j+1));
                j++;
            }
        }
     //   System.out.println(str.substring(n-1));

	}

}
