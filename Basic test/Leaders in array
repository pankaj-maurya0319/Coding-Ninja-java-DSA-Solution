public class Solution {

	public static void leaders(int[] arr) {
		
        int n = arr.length;
        
        boolean check = false;
        int max = arr[n-1];
        
        for(int i=n-1; i>=1; i--){
            
            if(arr[i] >= max ){
                max = arr[i];
                System.out.print(arr[i] + " ");
            }
            
        }
		
	}
	
}
